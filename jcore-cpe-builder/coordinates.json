                "default": "false"},
                {"name": "NegativeList",
                "type": "string",
                "default": ""},
                {"name": "ConsistencyPreservation",
                "type": "string",
                "default": "string,full2acro,acro2full"},
                {"name": "IgnoreNotIntroducedAbbreviations",
                "type": "boolean",
                "default": "true"}
